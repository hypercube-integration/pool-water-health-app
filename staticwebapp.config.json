{
  "routes": [
    { "route": "/admin/*", "allowedRoles": ["admin", "manager"] }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/favicon.ico",
      "/assets/*"
    ]
  },
  "responseOverrides": {
    "401": { "redirect": "/.auth/login/aad", "statusCode": 302 }
  }
}
